{
  "name": "eletrofeu-player",
  "version": "2.0.0",
  "description": "Eletrofeu Troféu Player - Versão reestruturada",
  "productName": "Eletrofeu Player",
  "author": "Eletrofeu Team",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "build": "quasar build",
    "build:electron": "quasar build -m electron",
    "build:electron:linux": "quasar build -m electron --target linux --bundler builder",
    "build:electron:linux:appimage": "quasar build -m electron --target linux --bundler builder --arch x64 --packager appimage",
    "build:electron:linux:deb": "quasar build -m electron --target linux --bundler builder --arch x64 --packager deb",
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@capacitor/app": "^7.0.1",
    "@quasar/extras": "^1.16.4",
    "pinia": "^2.1.3",
    "quasar": "^2.12.0",
    "video.js": "^8.3.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@capacitor/android": "^7.2.0",
    "@capacitor/cli": "^7.2.0",
    "@capacitor/core": "^7.2.0",
    "@quasar/app-vite": "^1.4.3",
    "autoprefixer": "^10.4.14",
    "electron": "^25.2.0",
    "electron-builder": "^24.4.0",
    "electron-packager": "^17.1.1",
    "eslint": "^8.44.0",
    "eslint-plugin-vue": "^9.15.1",
    "postcss": "^8.4.25",
    "workbox-build": "7.0.x",
    "workbox-cacheable-response": "7.0.x",
    "workbox-core": "7.0.x",
    "workbox-expiration": "7.0.x",
    "workbox-precaching": "7.0.x",
    "workbox-routing": "7.0.x",
    "workbox-strategies": "7.0.x"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
